{
  "name": "react-router-dispatcher",
  "version": "1.0.0-beta.6",
  "description": "Async server-side data loader using redux with react-router v4.x.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=4"
  },
  "repository": "git+ssh://git@github.com/adam-26/react-router-dispatcher.git",
  "scripts": {
    "build": "rm -rf ./lib; NODE_ENV=production babel ./src -d lib --ignore '__tests__'",
    "lint": "eslint ./src",
    "ci-jest": "TEST_REPORT_PATH=./.build/test jest --ci --coverage --coverageDirectory ./.build/coverage --testResultsProcessor='./node_modules/jest-junit-reporter'",
    "ci-lint": "eslint ./src --format junit --output-file ./.build/lint/eslint.xml",
    "test": "npm run lint && jest",
    "release": "standard-version --prerelease beta --no-verify"
  },
  "keywords": [
    "redux",
    "react",
    "connect",
    "async",
    "props"
  ],
  "author": "adam-26",
  "contributors": [
    "Vitaly Aminev <v@makeomatic.ru>",
    "Rodion Salnik (http://brocoders.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adam-26/react-router-dispatcher/issues"
  },
  "homepage": "https://github.com/adam-26/react-router-dispatcher",
  "peerDependencies": {
    "prop-types": "~15.x.x",
    "react": "15.x.x",
    "react-redux": "~4.x.x || ~5.x.x",
    "react-router-config": "^1.0.0-beta.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "bluebird": "^3.4.6",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0",
    "sinon": "^2.1.0",
    "standard-version": "^4.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "invariant": "^2.2.2",
    "redux-actions": "^2.0.1"
  },
  "jest": {
    "automock": false,
    "mapCoverage": true,
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  }
}
